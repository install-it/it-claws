{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "group": {
        "type": "string"
      },
      "path": {
        "type": "string"
      },
      "url": {
        "type": "string"
      },
      "file_type": {
        "type": "string",
        "enum": [
          "exe",
          "zip",
          "zip/folder",
          "zip/exe"
        ]
      },
      "rename_as": {
        "type": [
          "string",
          "null"
        ]
      }
    },
    "required": [
      "group",
      "path",
      "url",
      "file_type"
    ],
    "additionalProperties": false
  }
}